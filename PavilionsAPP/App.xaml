<Application x:Class="PavilionsAPP.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PavilionsAPP"
             xmlns:vm="clr-namespace:PavilionsAPP.ViewModel"
             xmlns:v="clr-namespace:PavilionsAPP.View"
             >
    <Application.Resources>



        <DataTemplate DataType="{x:Type vm:SignInVM}">
            <v:SignInPage />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:SignOnVM}">
            <v:SignOnPage />
        </DataTemplate>

    </Application.Resources>
</Application>
